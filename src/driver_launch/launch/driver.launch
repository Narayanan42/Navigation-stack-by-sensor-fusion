<launch>

    <arg name="port0" default="/dev/ttyUSB0"/>
    <node pkg="gps_driver" type="driver.py" name="gps">
        <param name="~port0" value="$(arg port0)"/>
    </node>
    
    <arg name="port1" default="/dev/ttyUSB1"/>
    <node pkg="imu_driver" type="driver.py" name="imu">
        <param name="~port1" value="$(arg port1)"/>
    </node>

</launch>
